
generator client {
    provider = "prisma-client-js"
    output = "./src/generated/client"
}

//Configuração do DB
datasource db {
    provider = "sqlite"
    url = "file:./dev.db"
}

// Model da tabeala?
model Task {
    id Int @id @default(autoincrement())
    title String
    details String
    deadline String
    status String
}